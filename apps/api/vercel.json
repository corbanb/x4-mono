{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": null,
  "installCommand": "bun install",
  "buildCommand": "bun turbo build --filter=@x4/api",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.ts"
    }
  ],
  "functions": {
    "api/index.ts": {
      "runtime": "nodejs22.x",
      "maxDuration": 10
    }
  },
  "ignoreCommand": "npx turbo-ignore"
}
